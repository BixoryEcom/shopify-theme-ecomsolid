<section class="gt_section-uzGeyIt9XBxvyg3 " data-name="Contact Us">
    <div class="gt_contact">
        <div class="gt_container">
          <div class="title_section gt_mb--medium">
            <h2 class="gt_heading h2_title">
              More Questions? <br>Send Us A Message
            </h2>
          </div>
           
             
                {% form 'contact' %}
            
            <div class="gf_flex gf_flex-wrap">
               <div class="gt_contact-left gt_mb--medium">
                <label>
                  Name: 
                </label>
                <input class="gt_input" type="text" id="ContactFormName" name="contact[name]" autocapitalize="words" value="{% if form.name %}{{ form.name }}{% elsif customer %}{{ customer.name }}{% endif %}">
              </div>
              <div class="gt_contact-right gt_mb--medium">
                  <label>
                    Email: 
                  </label>
                  <input class="gt_input" type="email" id="ContactFormEmail" name="contact[email]" autocorrect="off" autocapitalize="off" value="{% if form.email %}{{ form.email }}{% elsif customer %}{{ customer.email }}{% endif %}">
              </div>
              <div class="gt_contact-col gt_mb--medium gt_mb--medium">
                <label>
                  Phone Number: 
                </label>
                <input class="gt_input" type="tel" id="ContactFormPhone" name="contact[phone]" autocorrect="off" autocapitalize="off" value="{% if form.phone %}{{ form.phone }}{% elsif customer %}{{ customer.phone }}{% endif %}">
              </div>
              <div class="gt_contact-col gt_mb--medium">
                  <label>
                    Your Message: 
                  </label>
                  <textarea class="gt_input--large" name="contact[body]" id="ContactFormMessage" cols="30" rows="5">{% if form.body %}{{ form.body }}{% endif %}</textarea>
              </div>
            </div>
            <div class="error-mess">
              
              
                {% if form.posted_successfully? %}
                    <p class="success">
                      Success! Your message has been sent. 
                    </p>
                {% endif %}
                {% if form.errors %}
                    {% for field in form.errors %}
                        <p class="fail">{{ field }} {{ form.errors.messages[field] }}</p>
                    {% endfor %}
                {% endif %}
            </div>
            <button class="gt_button gt_contact-btn gt_pt--small gt_pb--small gt_pl--medium gt_pr--medium" type="submit">
              Send
            </button>
             
            {% endform %}
            
        </div>
    </div>
</section>

 
